<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.it.payment.mapper.JzhApiRequestsDao" >
	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.it.payment.entity.JzhApiRequests" id="jzhApiRequestsResultMap">
                <result property="id" column="id"/>
              <result property="seqNo" column="seq_no"/>
              <result property="requestUri" column="request_uri"/>
              <result property="requestContent" column="request_content"/>
              <result property="responseContent" column="response_content"/>
              <result property="retCode" column="ret_code"/>
              <result property="createdAt" column="created_at"/>
              <result property="updatedAt" column="updated_at"/>
          </resultMap>
    
  <sql id="Base_Column_List">
                  `id`,
                            `seq_no`,
                            `request_uri`,
                            `request_content`,
                            `response_content`,
                            `ret_code`,
                            `created_at`,
                            `updated_at`          </sql>
 
  <select id="queryList" resultMap="jzhApiRequestsResultMap">
    select
      <include refid="Base_Column_List" />
    from  jzh_api_requests
  </select>
  
	 
	<insert id="insert" parameterType="com.it.payment.entity.JzhApiRequests" useGeneratedKeys="true" keyProperty="id">
		insert into jzh_api_requests
		(
			`seq_no`,
			`request_uri`,
			`request_content`,
			`response_content`,
			`ret_code`,
			`created_at`,
			`updated_at`
		)
		values
		(
			#{seqNo},
			#{requestUri},
			#{requestContent},
			#{responseContent},
			#{retCode},
			#{createdAt}, 
			#{updatedAt}
		)
	</insert>
	 
	<update id="update" parameterType="com.it.payment.entity.JzhApiRequests">
		update jzh_api_requests
		<set>
			<if test="seqNo != null">`seq_no` = #{seqNo}, </if>
			<if test="requestUri != null">`request_uri` = #{requestUri}, </if>
			<if test="requestContent != null">`request_content` = #{requestContent}, </if>
			<if test="responseContent != null">`response_content` = #{responseContent}, </if>
			<if test="retCode != null">`ret_code` = #{retCode}, </if>
			<if test="createdAt != null">`created_at` = #{createdAt}, </if>
			<if test="updatedAt != null">`updated_at` = #{updatedAt}</if>
		</set>
		where id = #{id}
	</update>

	<select id="queryBySsn" resultMap="jzhApiRequestsResultMap" parameterType="String">
		select
		<include refid="Base_Column_List" />
		from  jzh_api_requests where request_content like '%${_parameter}%'
	</select>

</mapper>